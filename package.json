    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^17.2.3",
    "storybook-readme": "^4.0.2",
    "ts-loader": "^5.3.0",
